module RogueScript

class NativeRandom [singleton]
  METHODS
    method register( vm:VM )
      vm.register_type( "Random", (obj)=>obj.object instanceOf Random, "Object",
        ()=>Random, (v)=>Random=v.object->(as Random), &variant_type=Variant.TYPE_OBJECT )
      vm.register_method( "Random.int->Int", ()=>Random.int )
      vm.register_method( "Random.int32->Int", ()=>Random.int32 )
      vm.register_method( "Random.real->Real", ()=>Random.real )
endClass
