module Boss

class BossAdapted [aspect]
  PROPERTIES
    boss_type       : Type
    boss_properties = [String:Variant]

  METHODS
    method boss_properties->[String:Variant]: return @boss_properties
    method boss_type->Type: return @boss_type

    method set_boss_properties( @boss_properties )
    method set_boss_type( @boss_type )
endClass

class BossObject : BossAdapted
  METHODS
    method init( boss_type )
endClass
